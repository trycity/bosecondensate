// Phi.in

/*
class used as a wrapper to create the 1-body external potential wavefunction term
*/

#ifndef _PHI_H
#define _PHI_H

#include "@BUILD_DIR@/Utilities/VMCPoint.h"

namespace QMC{
class Phi
{
public:
   virtual ~Phi() {}

   virtual void setParam(double alpha){}
   virtual double operator()(const VMCPoint& x)const
   {
     return 1.0;
   }

};
};

#endif 
